{
  "name": "angular2-cli-test",
  "version": "0.0.0",
  "license": "MIT",
  "main": "server/dist/app.js",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",

    "server": "concurrently \"tsc -w -p ./server\" \"nodemon ./server/bin/www\" ",
    "develop": "concurrently \"tsc -w -p ./server\" \"tsc -w -p ./src\" \"nodemon ./server/bin/www\" ",
    "forever": "tsc -p ./server && forever start ./server/bin/www --watch",
    "twc:src:w": "tsc -w -p ./src",
    "twc:server:w": "tsc -w -p ./server",
    "typings": "typings install",
    "bundle:prod": "node tools/builder.js --prod",
    "postinstall": "typings install && tsc -p ./server"
  },
  "private": true,
  "engines": {
    "node": ">= 5.4.1"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.23",

    "angular2-in-memory-web-api": "0.0.20",
    "angular2-jwt": "^0.1.22",
    "ng-semantic": "^1.1.3",
    "yargs": "^4.8.1"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.32",
    "@types/express": "^4.0.32",
    "@types/jsonwebtoken": "^7.1.31",
    "@types/serve-favicon": "^2.2.27",
    "debug": "^2.2.0",
    "forever": "^0.15.2",
    "serve-favicon": "~2.3.0",
    "express-jwt": "^3.3.0",
    "reflect-metadata": "^0.1.3",
    "concurrently": "^2.2.0",
    "nodemon": "^1.9.1",
    "jsonwebtoken": "^5.7.0",

    "@types/jasmine": "^2.2.30",
    "angular-cli": "1.0.0-beta.15",
    "codelyzer": "~0.0.26",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.5",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "2.0.2"
  }
}